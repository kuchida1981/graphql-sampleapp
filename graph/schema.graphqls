# GraphQL schema for Hello World sample

type Query {
  hello: String!
  messages: [Message!]!
  message(id: ID!): Message
  users: [User!]!
  user(id: ID!): User
}

type Message {
  id: ID!
  content: String!
  author: String!
  createdAt: String!
}

type User {
  id: ID!
  name: String!
  email: String!
  createdAt: String!
}
